	"version": "0.2.0",
	"configurations": [
		{
			"name": "markdown-notes-mcp",
			"type": "python",
			"request": "launch",
			"program": "markdown-notes-mcp.py",
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"justMyCode": true,
			"args": [
				"-c",
				"${workspaceFolder}/etc/markdown-notes-mcp.conf"
			],
			"env": {
				"PYTHONPATH": "/root/Workspace/asab:%{PYTHONPATH}"
			}
		}
	]
}